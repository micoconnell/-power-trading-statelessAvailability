{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "functions": ["OpenInterestScrape","InterchangesDbUpdate","TransmissionMasterDEV","SMPTrigger","SevenDayPROD","StackModelGroupingGas","UpdateMonthlyAgainstKnown","PriceTrigger","TransmissionMaster","dailyDrone","dailyMaster","MonthlyDrone","MonthlyMaster","sevendaydrone","sevendaymaster","TransmissionDrone","MonthlyOutageSQLUpdate", "DailyOutageSQLUpdate" ],
  "functionTimeout": "02:00:00",
  "healthMonitor": {
      "enabled": false,
      "healthCheckInterval": "00:00:10",
      "healthCheckWindow": "00:02:00",
      "healthCheckThreshold": 6,
      "counterThreshold": 0.80
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  }
}

